    "ws": "^7.4.3"
  },
  "types": "./typings/index.d.ts",
  "devDependencies": {
    "@splatterxl/tsconfig": "^1.0.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
