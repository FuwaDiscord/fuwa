    "ws": "^8.0.0"
  },
  "types": "./typings/index.d.ts",
  "devDependencies": {
    "@splatterxl/tsconfig": "1.0.0",
    "@types/mocha": "9.0.0",
    "@types/node": "14.14.41",
    "@types/node-fetch": "2.5.10",
    "@types/ws": "7.4.4",
