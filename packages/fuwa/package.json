{
  "name": "fuwa",
  "version": "0.1.0",
  "description": "fuwa is a simple, easy-to-use and lightweight Discord API wrapper",
  "main": "./dist/index.js",
  "type": "commonjs",
  "scripts": {
    "precommit": "pnpm build --noEmit && pnpm lint",
    "lint": "eslint . --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "build": "tsc",
    "prettify": "prettier -w ."
  },
  "repository": "https://github.com/fuwa-org/fuwa.git",
  "keywords": [],
  "author": "Splatterxl <splatterxl@outlook.ie>",
  "license": "MIT",
  "types": "./typings/index.d.ts",
  "homepage": "https://fuwadiscord.github.io/fuwa/",
  "dependencies": {
    "@fuwa/rest": "workspace:^",
    "@fuwa/ws": "workspace:^",
    "@sapphire/async-queue": "1.5.0",
    "@types/ws": "8.5.5",
    "discord-api-types": "0.37.46",
    "lodash.snakecase": "4.1.1",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@splatterxl/tsconfig": "1.1.1",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.43.0",
    "prettier": "2.8.8",
    "standard-version": "9.5.0",
    "typedoc": "0.24.8",
    "typescript": "5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "optionalPeerDependencies": {}
}
